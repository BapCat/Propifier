{
  "name": "bapcat/propifier",
  "description": "A trait that improves PHP's properties",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/BapCat/Propifier",
  "keywords": ["Property", "Properties", "Accessor", "Mutator", "Get", "Set", "Getter", "Setter"],
  "authors": [
    {
      "name": "Corey Frenette",
      "email": "corey@narwhunderful.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "icanboogie/inflector": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "BapCat\\Propifier\\": "src/"
    }
  }
}
